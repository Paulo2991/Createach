{
  "name": "ravendb-cloud",
  "version": "1.0.2",
  "description": "RavenDB Cloud API client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://cloud.ravendb.net",
  "scripts": {
    "build": "mkdirp dist && tsc",
    "test": "mkdirp .test && tsc -p tsconfig.test.json && mocha .test\\test",
    "test-debug": "mkdirp .test && tsc -p tsconfig.test.json && mocha --inspect-brk .test\\test",
    "clean": "rimraf .test && rimraf dist",
    "codegen": "cd .. && powershell.exe ./generate_typescript_client.ps1 && cd typescript"
  },
  "files": [
    "dist/"
  ],
  "bugs": {
    "email": "support@ravendb.net",
    "url": "https://issues.hibernatingrhinos.com"
  },
  "contributors": [
    "Hibernating Rhinos"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "ravendb",
    "cloud",
    "ravendb-cloud",
    "api",
    "typescript",
    "node",
    "nodejs",
    "nosql",
    "document",
    "database",
    "aws",
    "azure",
    "gcp"
  ],
  "author": "Hibernating Rhinos",
  "license": "MIT",
  "devDependencies": {
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "node-fetch": "^2.6.1"
  }
}
